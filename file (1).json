interface Theme {
/** Theme's name */
"name": "Vinland Saga";
/** Theme's description */
"description": "VS theme with bg from Reddit";
/** Array of theme authors */
authors: {
/** Author's nickname */
"name": "TightCount";
/** Author's Discord user ID */
"id": "tightcount";
}[];
/** Theme version (currently 2) */
"spec": 1;
/** Background that shows up in chat */
background: {
/** Determines the blurriness of the background (goes from 0-1) */
blur: number;
/** A link to the background */
"url": "https://cdn.discordapp.com/attachments/906894277861339146/1288792512680431628/new-phone-wallpaper-reformatted-for-phone-by-ai-v0-rahkduti279b1.jpg?ex=66f6790f&is=66f5278f&hm=678f0ab6619604adee5bbffffa5afc57ecc6dcd86f0a8d841661053c9ccd5858&";
/** Determines the opacity of the background (goes from 0-1, affects color too) */
alpha: number;
};
}